<html>

<head>
<link rel="stylesheet" href="style.css">
<script>
function myFunction(){


var canvas = document.getElementById('canvas');
var ctx = canvas.getContext('2d');

ctx.beginPath();
ctx.moveTo(0, 240);
ctx.lineTo(640, 240);
ctx.stroke();

ctx.beginPath();
ctx.moveTo(320, 0);
ctx.lineTo(320,480);
ctx.stroke();

ctx.strokeRect(0, 0, 300, 230);


ctx.strokeRect(0, 225, 640, 35);


ctx.strokeRect(330, 200, 10, 250);

ctx.strokeRect(330, 200, 200, 20);

ctx.strokeRect(340, 20, 300, 220);
ctx.strokeRect(330, 250, 200, 200);

ctx.rect(340, 0, 400, 230);
ctx.fillStyle = 'green';
ctx.fill();
ctx.rect(340, 255, 400, 180);
ctx.fillStyle = 'green';
ctx.fill();


ctx.strokeRect(330, 250, 20, 10);



}
</script>
</head>
<body onload="myFunction()">
<canvas id="canvas" width="640" height="480"></canvas>

</body>

</html>>
