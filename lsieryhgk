<html>

<head>
<link rel="stylesheet" href="style.css">
<script>
function myFunction(){


var canvas = document.getElementById('canvas');
var ctx = canvas.getContext('2d');

ctx.beginPath();
ctx.moveTo(0, 240);
ctx.lineTo(640, 240);
ctx.stroke();

ctx.beginPath();
ctx.moveTo(320, 0);
ctx.lineTo(320,480);
ctx.stroke();

ctx.strokeRect(0, 0, 310, 235);
}
</script>
</head>
<body onload="myFunction()">
<canvas id="canvas" width="640" height="480"></canvas>

</body>

</html>
